            {
                "Name": "runTranslate",
                "Params":
                {
                    "Credentials": "Default",
                    "L1": "en",
                    "L2": "id",
                    "SearchOneByOne": "False"
                }
            }
        ]
        },
        {
        "Name": "Browser",
